{
  "rewrites": [
    {
      "source": "/blocks/:path*",
      "destination": "/blocks/:[path*].tsx"
    },
    {
      "source": "/block/:path*",
      "destination": "/block/:[path*].tsx"
    },
    {
      "source": "/txn/:path*",
      "destination": "/txn/:[path*].tsx"
    },
    {
      "source": "/account/:path*",
      "destination": "/account/:[path*].tsx"
    }
  ],
  "trailingSlash": true,
  "query": {
    "network": ":network"
  }
}
